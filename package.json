{
    "name": "vsmud",
    "displayName": "vsmud",
    "description": "Mud客户端",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.99.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:vmud",
        "onFileSystem:*.vmud"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "languages": [
            {
                "id": "vmud",
                "extensions": [
                    ".vmud"
                ],
                "aliases": [
                    "vmud"
                ]
            }
        ],
        "customEditors": [
            {
                "viewType": "vsmudClient",
                "displayName": "Mud客户端",
                "selector": [
                    {
                        "filenamePattern": "*.vmud"
                    }
                ]
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "webpack",
        "compile-watch": "set NODE_ENV=development && webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "npm run compile-tests && npm run compile && npm run lint",
        "lint": "eslint src",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/mocha": "^10.0.10",
        "@types/node": "20.x",
        "@types/vscode": "^1.99.0",
        "@typescript-eslint/eslint-plugin": "^8.28.0",
        "@typescript-eslint/parser": "^8.28.0",
        "@vscode/test-cli": "^0.0.10",
        "@vscode/test-electron": "^2.4.1",
        "eslint": "^9.23.0",
        "iconv-lite": "^0.6.3",
        "ts-loader": "^9.5.2",
        "typescript": "^5.8.2",
        "webpack": "^5.98.0",
        "webpack-cli": "^6.0.1"
    }
}
